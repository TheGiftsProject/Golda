{
  "name": "My Second Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "golda.jpg"
  },
  "icons": { "16": "golda.jpg"
            },
  "background": {
    "scripts": ["jquery.js","background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": 
  [{
    "matches": ["<all_urls>"],
    "js": ["jquery.js", "domget.js"]
  }]
}

