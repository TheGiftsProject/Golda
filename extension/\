	<script type="text/javascript">	
		function goTo214(txName){
			var selectedBerur = document.getElementById("selectedBerur");
			var selectedBerurVal = selectedBerur.value;
			var selectedBerurText = selectedBerur[selectedBerur.selectedIndex].text;

			if (selectedBerurVal == "0"){
				alert("נא לבחור סיבת בירור.");
				return;
			}else if (selectedBerurVal == "2" || selectedBerurVal == "509"){
				goTo205();
			}else {		
				document.getElementById("selectedBerurText").value = selectedBerurText;
	   			document.getElementById("stage").value = "0";
	   			document.getElementById("transactionId").value = txName;
	   			document.getElementById("reqName").value = "action";
				document.getElementById("iform").submit(); 
			}
	  	}
	  	
		function performTransaction(txName){ 
     		document.getElementById("showAsFile").value = '0';
			document.getElementById("transactionId").value = txName;
			document.getElementById("reqName").value = "action";
			document.getElementById("iform").submit();
		}
		
		function goTo205(){
   			document.getElementById("reqName").value = "action";
   			document.getElementById("stage").value = "1";
			goForward("GetCopyOfShovarIska_205", "PirteyIska_204");   
	  	}
	</script>

